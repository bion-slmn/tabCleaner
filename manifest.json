{
    "manifest_version": 3,
    "name": "Tab manager",
    "version": "1.0",
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    }
}